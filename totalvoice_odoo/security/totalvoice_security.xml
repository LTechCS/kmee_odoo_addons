<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record model="ir.module.category" id="module_category_totalvoice">
            <field name="name">Totalvoice</field>
            <field name="description">User access level for this module</field>
            <field name="sequence">9</field>
        </record>

        <record id="group_totalvoice_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" ref="module_category_totalvoice"/>
        </record>

        <record id="group_totalvoice_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="category_id" ref="module_category_totalvoice"/>
            <field name="implied_ids" eval="[(4, ref('group_totalvoice_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
        </record>

    </data>
</odoo>


