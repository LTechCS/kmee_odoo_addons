<openerp>
    <data>

        <report
            id="report_contract_py3o"
            string="Ficha de Registro de Funcionario"
            model="hr.contract"
            report_type="py3o"
            name="l10n_br_hr_contract_report.report_contract"
        />

        <record id="py3o_template_contract_ficha_registro"
                model="py3o.template">
            <field name="name">Ficha</field>
            <field name="filetype">odt</field>
            <field name="py3o_template_data" type="base64"
                   file="l10n_br_hr_contract_report/data/registro_funcionario.odt"></field>
        </record>

        <record id="registro_funcionario" model="ir.actions.report.xml">
            <!--<field name="report_type">py3o</field>-->
            <field name="module">l10n_br_hr_contract_report</field>
            <field name="py3o_fusion_filetype" ref="report_py3o.py3o_fusion_filetype_odt"/>
            <field name="py3o_template_fallback">data/registro_funcionario.odt</field>
            <field name="py3o_template_id" ref="py3o_template_contract_ficha_registro"/>
        </record>

    </data>
</openerp>
